<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/swim_items"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="10dp"
    android:layout_marginHorizontal="20dp"
    android:background="@drawable/sh_swim_item_card"
    android:padding="10dp"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/swim_items_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif"
        android:gravity="center_horizontal"
        android:text=""
        android:textAlignment="center"
        android:textColor="?attr/secondaryColor"
        android:textSize="30sp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="10dp" />

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintVertical_weight="1"
        android:baselineAligned="false"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/swim_items_name"
        tools:layout_editor_absoluteX="10dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/swim_items_p1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:id="@+id/swim_items_d1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text=""
                android:textColor="?attr/textColor"
                android:textSize="@dimen/swim_item_size_time"
                app:layout_constraintBottom_toBottomOf="@id/swim_items_d2"
                app:layout_constraintEnd_toEndOf="@+id/swim_items_d2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_weight="1" />

            <TextView
                android:id="@+id/swim_items_d2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text=""
                android:textColor="?attr/textColor"
                android:textSize="@dimen/swim_item_size_time"
                app:layout_constraintBottom_toBottomOf="@id/swim_items_d3"
                app:layout_constraintEnd_toEndOf="@id/swim_items_d3"
                app:layout_constraintStart_toStartOf="@id/swim_items_d1"
                app:layout_constraintTop_toBottomOf="@+id/swim_items_d1"
                app:layout_constraintVertical_weight="1" />

            <TextView
                android:id="@+id/swim_items_d3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintVertical_weight="1"
                android:gravity="center"
                android:text=""
                android:textColor="?attr/textColor"
                android:textSize="@dimen/swim_item_size_time"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/swim_items_d2"
                app:layout_constraintTop_toBottomOf="@+id/swim_items_d2" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/swim_items_p2"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:id="@+id/swim_items_d4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text=""
                android:textColor="?attr/textColor"
                android:textSize="@dimen/swim_item_size_time"
                app:layout_constraintBottom_toBottomOf="@id/swim_items_d5"
                app:layout_constraintEnd_toEndOf="@+id/swim_items_d5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_weight="1" />

            <TextView
                android:id="@+id/swim_items_d5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="?attr/textColor"
                android:textSize="@dimen/swim_item_size_time"
                app:layout_constraintBottom_toBottomOf="@id/swim_items_d6"
                app:layout_constraintEnd_toEndOf="@id/swim_items_d6"
                app:layout_constraintStart_toStartOf="@id/swim_items_d4"
                app:layout_constraintTop_toBottomOf="@+id/swim_items_d4"
                app:layout_constraintVertical_weight="1" />

            <TextView
                android:id="@+id/swim_items_d6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text=""
                android:textColor="?attr/textColor"
                android:textSize="@dimen/swim_item_size_time"
                app:layout_constraintVertical_weight="1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/swim_items_d5"
                app:layout_constraintTop_toBottomOf="@+id/swim_items_d5" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>